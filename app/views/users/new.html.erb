<h1>新規登録</h1>
<%= form_with model: @user, url: users_create_path do |f| %>
    <label for="name">name</label>
    <%= f.text_field :name %>
 
    <label for="email">email</label>
    <%= f.text_field :email %>
  
    <label for="password">password</label>
    <%= f.text_field :password %>
    <%= f.submit :登録 %>
<% end %>