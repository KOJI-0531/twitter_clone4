<h1>ログイン</h1>
<%= form_with model: User.new , url: users_login_path(@user) do |f| %>
    <label for="name">name</label>
    <%= f.text_field :name %>
    
    <label for="email">email</label>
    <%= f.text_field :email %>
    
    <label for="password">password</label>
    <%= f.text_field :password %>
    <%= f.submit :ログイン %>
<% end %>